{
  "name": "meltdown-tracker-typescript",
  "version": "1.0.0",
  "description": "A journal app to help parents of children with autism to track frequency of meltdowns along with potential environmental causes",
  "main": "index.ts",
  "scripts": {
    "start": "node ./server/index.ts",
    "dev": "nodemon ./server/index.ts",
    "build": "npm install && cd client && npm install && npm run build",
    "make:migration": "knex migrate:make",
    "migrate": "knex migrate:latest",
    "make:seed": "knex seed:make",
    "seed": "knex seed:run"
  },
  "keywords": [
    "autism",
    "journal",
    "meltdown",
    "tracker",
    "asd"
  ],
  "author": "Nicole Boci",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.10",
    "nodemon": "^3.0.1"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "knex": "^3.0.1",
    "pg": "^8.11.3"
  }
}
